{
    "name": "nodes/database",
    "description": "Makes it easier to work with models and repositories in Laravel",
    "keywords": [
        "nodes",
        "database",
        "laravel",
        "eloquent",
        "repository"
    ],
    "license": "MIT",
    "homepage": "http://nodesagency.com",
    "authors": [
        {
            "name": "Morten Rugaard",
            "email": "moru@nodes.dk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^9.0",
        "illuminate/database": "^9.0",
        "nodes/core": "dev-php-8-1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6.7",
        "mockery/mockery": "^1.4",
        "orchestra/testbench": "^v7.0"
    },
    "suggest": {
        "nodes/counter-cache": "Adds the concept of counter caching to your project"
    },
    "autoload": {
        "psr-4": {
            "Nodes\\Database\\": "src/"
        },
        "files": [
            "src/Support/Helpers/Query.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Nodes\\Database\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jamesforddesign/core"
        }
    ]
}
